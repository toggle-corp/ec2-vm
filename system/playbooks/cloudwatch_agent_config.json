{
  "logs": {
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "/var/lib/docker/containers/*/*.log",
            "log_group_name": "docker-container-logs",
            "log_stream_name": "{instance_id}"
          }
        ]
      }
    }
  }
}

